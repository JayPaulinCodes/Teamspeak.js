{
    "name": "teamspeak.js",
    "version": "0.1.0",
    "description": "A package for connecting and interacting with Teamspeak3 servers using Server Query",
    "authors": [
        "SparrowStudios",
        "Jacob Paulin"
    ],
    "license": "Mozilla Public License Version 2.0",
    "main": "lib/index.js",
    "homepage": "https://github.com/SparrowStudios/Teamspeak.js#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SparrowStudios/Teamspeak.js.git"
    },
    "scripts": {
        "lint": "prettier --check .",
        "format": "prettier --write .",
        "clean-install-dev": "npm ci",
        "clean-install-prod": "npm ci --omit=dev",
        "prebuild": "npm run clean-install-prod",
        "build": "tsc",
        "prebuild-dev": "npm run clean-install-dev",
        "build-dev": "tsc",
        "poc": "npm run build-dev && node devPoc/index.js",
        "poc-no-build": "node devPoc/index.js"
    },
    "keywords": [
        "ts",
        "ts3",
        "TS3",
        "Teamspeak",
        "Teamspeak3",
        "Teamspeak3 Server Query",
        "Server Query"
    ],
    "dependencies": {
        "@discordjs/collection": "^2.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.9.0",
        "prettier": "^3.1.0",
        "typedoc": "^0.25.3",
        "typedoc-github-wiki-theme": "^1.1.0",
        "typedoc-plugin-coverage": "^2.2.0",
        "typedoc-plugin-markdown": "^3.17.1",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=18"
    }
}
