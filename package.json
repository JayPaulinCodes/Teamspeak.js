    "devDependencies": {
        "@types/node": "^20.9.0",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "eslint": "^8.56.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "prettier": "^3.1.0",
        "typedoc": "^0.25.3",
        "typedoc-github-wiki-theme": "^1.1.0",
        "typedoc-plugin-coverage": "^2.2.0",
